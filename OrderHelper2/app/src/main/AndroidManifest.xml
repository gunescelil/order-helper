<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sp2016.cs310.com.orderhelper">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TablesActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ManagerActivity" />
        <activity android:name=".ManagersRestaurantPageActivity" />
        <activity android:name=".SeeTableActivity" />
        <activity android:name=".ShowCategoriesActivity" />
        <activity android:name=".ShowMenuDetailsActivity" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".ManageWaitersActivity" />
        <activity android:name=".ManageMealsActivity"></activity>
    </application>

</manifest>